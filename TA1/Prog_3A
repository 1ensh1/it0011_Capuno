rows = 5 

for i in range(1, rows + 1): #range niya is (1, 6) value is 1 to 5
    for j in range(rows - i): #inner loop
        print(" ", end="") #print ng spaces. Since rows - 1, print niya is 5 - 1 = 4 spaces

    for j in range(1, i + 1): #inner inner loop
        print(j, end="") #after ng spaces nagp-print yung loop ng numbers starting from 1 to i

    print()  #kada row minomove niya yung cursor sa next line
